    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "polished": "^3.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "rimraf": "^2.6.3",

{
  "name": "personal_site_3",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:app\" \"yarn dev:server\"",
    "storybook": "start-storybook --ci -p 5001 -c config/storybook",
    "dev:app": "webpack-dev-server --config config/webpack/frontend.dev",
    "dev:server": "nodemon server/index.js --inspect=0.0.0.0:9229 --exec babel-node",
    "prod:build": "yarn prod:clear && webpack --config config/webpack/frontend.prod && webpack --config config/webpack/backend.prod",
    "start": "yarn prod:build && yarn prod:run",
    "prod:run": "node public/dist/bundle-backend.js",
    "prod:clear": "rimraf ./public/dist/*"
  },
  "author": "",
  "license": "ISC",
